   "source": [
    "def rabbits(n, k=1):\n",
    "    if n == 1:\n",
    "        return 1\n",
    "    if n == 2:\n",
    "        return 1\n",
    "    return rabbits(n-1,k) + rabbits(n-2,k)*k"
   ]
